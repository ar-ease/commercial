generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Product {
  id          String         @id @default(cuid())
  name        String      @unique
  image       String      @default("data:image/jpeg;base64,")
  description String?     
  productType ProductType 
  available   Available   @default(INSTOCK)
  createdAt   DateTime    @default(now())
  updatedAt   DateTime    @updatedAt
}

enum ProductType {        
  PAPER                  
  BOARD
}

enum Available {
  INSTOCK
  OUTOFSTOCK
}
